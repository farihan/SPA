<table class="ko-grid">
    <thead>
        <tr>
            <th>No.</th>
            <th>Product Name</th>
            <th>Quantity per Unit</th>
            <th>Unit Price</th>
            <th>Units in Stock</th>
            <th>Units on Order</th>
            <th>Reorder Level</th>
            <th>Discontinued</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: itemsOnCurrentPage">
        <tr>
            <td data-bind="text: $root.itemNumber($root.pageSize * $root.currentPageIndex() + $index())"></td>
            <td data-bind="text: ProductName"></td>
            <td data-bind="text: QuantityPerUnit"></td>
            <td data-bind="text: UnitPrice"></td>
            <td data-bind="text: UnitsInStock"></td>
            <td data-bind="text: UnitsOnOrder"></td>
            <td data-bind="text: ReorderLevel"></td>
            <td><input type="checkbox" data-bind="checked: Discontinued" /></td>
        </tr>
    </tbody>
</table>